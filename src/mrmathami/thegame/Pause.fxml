<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mrmathami.thegame.Main">
    <children>
        <Button layoutX="411.0" layoutY="448.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="52.0" prefWidth="138.0" text="Back to Menu">
            <font>
                <Font name="Cooper Black" size="16.0" />
            </font>
        </Button>
        <Button layoutX="411.0" layoutY="371.0" mnemonicParsing="false" onAction="#continue_" prefHeight="52.0" prefWidth="138.0" text="Continue">
            <font>
                <Font name="Cooper Black" size="17.0" />
            </font>
        </Button>
        <Button layoutX="410.0" layoutY="524.0" mnemonicParsing="false" onAction="#quit" prefHeight="52.0" prefWidth="138.0" text="Quit">
            <font>
                <Font name="Cooper Black" size="15.0" />
            </font>
        </Button>
        <TextField alignment="CENTER" layoutX="342.0" layoutY="181.0" prefHeight="157.0" prefWidth="275.0" text="PAUSED !!">
            <font>
                <Font name="Cooper Black" size="32.0" />
            </font>
