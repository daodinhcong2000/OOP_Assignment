<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mrmathami.thegame.Main">
   <children>
      <ImageView fitHeight="640.0" fitWidth="960.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true" scaleY="1.2">
         <image>
            <Image url="@startmenu.png" />
         </image>
      </ImageView>
      <Button layoutX="359.0" layoutY="368.0" mnemonicParsing="false" onAction="#loadLevel1" prefHeight="74.0" prefWidth="242.0" text="New game">
         <font>
            <Font name="Cooper Black" size="22.0" />
         </font>
      </Button>
      <Button layoutX="359.0" layoutY="553.0" mnemonicParsing="false" onAction="#quit" prefHeight="74.0" prefWidth="242.0" text="Quit game">
         <font>
            <Font name="Cooper Black" size="22.0" />
         </font>
      </Button>
      <Button layoutX="359.0" layoutY="464.0" mnemonicParsing="false" onAction="#selectLevel" prefHeight="74.0" prefWidth="242.0" text="Select level">
         <font>
            <Font name="Cooper Black" size="22.0" />
         </font>
      </Button>
      <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutY="-15.0" mnemonicParsing="false" onAction="#setMute" opacity="0.0" prefHeight="67.0" prefWidth="74.0">
         <font>
            <Font name="Cooper Black" size="49.0" />
         </font>
      </Button>
      <Button layoutX="66.0" layoutY="-17.0" mnemonicParsing="false" onAction="#setSFX" opacity="0.0" prefHeight="66.0" prefWidth="90.0" text="SFX">
         <font>
            <Font name="Cooper Black" size="49.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
